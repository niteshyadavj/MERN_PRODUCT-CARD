
{
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      },
      {
        "src": "dist/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/",
        "dest": "dist/index.html"
      },{
        "src": "/product",
        "dest": "index.js"
      },
      {
        "src": "/product/(.*)",
        "dest": "index.js"
      },
      {
        "src": "/add",
        "dest": "dist/index.html"
      },
      
      {
        "src": "/(.+)",
        "dest": "dist/$1"
      }
      
    ]
  }
